{
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig.json to read more about this file */
    "types": [
      "node"
    ],
    "baseUrl": "src",
    "outDir": "./dist",
    "typeRoots": ["./node_modules/@types"],
    "noImplicitAny": false,
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true ,
    "skipLibCheck": true /* Skip type checking all .d.ts files. */
  }
}